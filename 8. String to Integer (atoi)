class Solution:
    def myAtoi(self, s: str) -> int:
        maxint=2147483647
        minint=-2147483648
        index=0
        strlen = len(s)
        sign=1
        result =0
        while index<strlen and s[index].isspace():
            index += 1
        if index < strlen and s[index]=="-":
            sign = -1
            index = index+1
        elif index<strlen and s[index]=="+":
            index += 1
            #sign remains positive
        
        while index<len(s) and s[index].isdigit():
            result = result*10 + int(s[index])
            index += 1
            if result > maxint or result < -maxint:
                return maxint if sign == 1 else minint
        return result*sign
            
