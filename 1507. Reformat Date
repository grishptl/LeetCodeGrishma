class Solution:
    def reformatDate(self, date: str) -> str:
        
        month={"Jan":"01", "Feb":"02", "Mar":"03", "Apr":"04", "May":"05", "Jun":"06", "Jul":"07", "Aug":"08", "Sep":"09", "Oct":"10", "Nov":"11", "Dec":"12"} #(O(12)) Space
        
        d= date.split() #constant to length of string
        return str(d[-1])+"-"+str(month.get(d[1]))+"-"+("%02d" % int(d[0][0:-2]))
        
        #Time: O(1) Space:O(1)
