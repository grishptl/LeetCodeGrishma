class Solution:
    def searchRange(self, nums: List[int], target: int) -> List[int]:
        leftptr = self.binarysearch(nums,target,True)
        rightptr = self.binarysearch(nums,target,False)
        return [leftptr,rightptr]
    def binarysearch(self,nums,target,leftsurf):
        left =0
        right = len(nums)-1
        ans=-1
        while left<=right:
            mid = (left+right)//2
            if nums[mid]<target:
                left = mid+1
            elif nums[mid]>target:
                right= mid-1
            else:
                ans = mid
                if leftsurf:
                    right = mid-1
                else:
                    left = mid+1
        return ans
