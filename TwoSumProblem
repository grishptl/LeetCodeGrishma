class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        idx = 0;
        for i in nums:
            anothernum = target-i
            if anothernum in nums[idx+1:]:
                numscopy = nums[idx+1:]
                return [nums.index(i),numscopy.index(anothernum)+idx+1]
            idx += 1   
        
