# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution:
    def getIntersectionNode(self, headA: ListNode, headB: ListNode) -> Optional[ListNode]:
        visitedset = set()
        
        while headA: #O(A)
            if headA in visitedset:
                return headA
            visitedset.add(headA)
            headA = headA.next
        while headB: #O(B)
            if headB in visitedset:
                return headB
            visitedset.add(headB)
            headB = headB.next
        return None
        
