#121. Best Time to Buy and Sell Stock
class Solution:
    def maxProfit(self, prices: List[int]) -> int:
        #two pointer approach O(n)
        maxprof=0
        left=0
        right=1
        while right<len(prices):
            if prices[left]<prices[right]:
                maxprof=max(prices[right]-prices[left],maxprof)
            else:
                left=right
            right = right+1
        return maxprof
