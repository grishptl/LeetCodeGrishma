# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution:
    def deleteNode(self, node):
        """
        :type node: ListNode
        :rtype: void Do not return anything, modify node in-place instead.
        """
        #1 copy content of the next node
        copynext = node.next.next
        copynextval = node.next.val
    
        
        #2. Delete node from step1
        node.next = copynext
        node.val = copynextval
                
