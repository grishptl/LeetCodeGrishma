class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        visitedmap = {}
        if len(s)<=1:
            return len(s)
        left = 0
        maxlen = 0
        for right in range(len(s)):
            if s[right] in visitedmap and left <= visitedmap[s[right]]:
                left = visitedmap[s[right]]+1
            else:
                maxlen = max(maxlen, right-left+1)
            visitedmap[s[right]] = right
        return maxlen
        
