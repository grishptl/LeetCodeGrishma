class Solution(object):
    def isPalindrome(self, x):
        """
        :type x: int
        :rtype: bool
        """
        """
        y = str(x)
        print(y[::-1])
        if y[::-1]==str(x):
            return True
        return False
    
        """
        if x<0 or (x%10==0 and x!=0):
            return False
        result = 0
        while x>result:
            print(x,x%10,result)
            result = result*10 + x%10
            x = x//10
        #print(x,result)
        return x==result or result//10 == x
